  "scripts": {
    "//1": " --- NPM Hooks --- ",
    "start": "npm run development",
    "test": "npm run test:js",
    "//2": " --- Build --- ",
    "watch:development": "NODE_ENV=development webpack-dev-server --config config/webpack/webpack.config.js --watch -d --env.NODE_ENV=development --env.WATCH=true --env.MODE=development",
    "build:development": "NODE_ENV=development webpack --config config/webpack/webpack.config.js -d --env.NODE_ENV=development --env.MODE=development",
    "build:test": "NODE_ENV=production webpack --config config/webpack/webpack.config.js -p --env.NODE_ENV=test --env.MODE=development",
    "build:production": "NODE_ENV=production webpack --config config/webpack/webpack.config.js -p --env.NODE_ENV=production --env.MODE=production",
    "flow": "flow",
    "//3": " --- Testing --- ",
    "test:js": "jest",
    "test:js:watch": "jest --watch",
    "test:js:coverage": "jest --coverage"
  },
